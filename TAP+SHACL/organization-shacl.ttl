        <organizationidentifiershapeOrganizationIdentifier>,
        <organizationidentifiershapeType> ;
    sh:targetObjectsOf ceds:P600502 .

<accreditationshapeAccrediationBodyCode> a sh:PropertyShape ;
    sh:datatype xsd:string ;
    sh:in ( "MSA NCA-HLC NCA-CASI NEASC-CIHE NEASC-CTCI NWCCU SACS WASC-ACCJC WASC-ACSCU Other" ) ;
    sh:maxCount 1 ;
    sh:name "Accrediation Body Code"@en-US ;
    sh:nodeKind sh:Literal ;
    sh:path pesc:accreditationBodyCode .

<accreditationshapeAccreditingBodyName> a sh:PropertyShape ;
    sh:datatype xsd:string ;
    sh:maxCount 1 ;
    sh:maxLength 80 ;
    sh:name "Accrediting Body Name"@en-US ;
    sh:nodeKind sh:Literal ;
    sh:path pesc:accreditingBodyName .

<accreditationshapeAdditionalNote> a sh:PropertyShape ;
    sh:name "Additional Note"@en-US ;
    sh:node <NoteShape> ;
    sh:nodeKind sh:BlankNodeOrIRI ;
    sh:path elm:additionalNote .

<organizationidentifiershapeHasOrganizationIdentificationSystem> a sh:PropertyShape ;
    sh:in ( ceds:NI000827151968 pescIdentifierTypes:OPEid pescIdentifierTypes:PSISid ) ;
    sh:maxCount 1 ;

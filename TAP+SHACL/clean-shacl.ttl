    sh:nodeKind sh:Literal ;
    sh:path pesc:nsn .

<studentshapeName> a sh:PropertyShape ;
    sh:maxCount 1 ;
    sh:minCount 1 ;
    sh:name "Name"@en-US ;
    sh:node <NameShape> ;
    sh:nodeKind sh:BlankNodeOrIRI ;
    sh:path ceds:hasPersonName .

<studentshapeNoteMessage> a sh:PropertyShape ;

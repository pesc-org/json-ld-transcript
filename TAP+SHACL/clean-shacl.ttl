<studentshapeFICEID> a sh:PropertyShape ;
    sh:datatype xsd:string ;
    sh:maxCount 1 ;
    sh:maxLength 6 ;
    sh:minLength 6 ;
    sh:name "FICE ID"@en-US ;
    sh:nodeKind sh:Literal ;
    sh:path pesc:ficeID .

<studentshapeGenderCode> a sh:PropertyShape ;

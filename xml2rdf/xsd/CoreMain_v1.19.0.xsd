				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="GovernmentMilitaryType">
		<xs:annotation>
			<xs:documentation>Additional information about the prior or current history of government or military affiliations </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="MilitaryRelationshipStatusCode" type="core:MilitaryRelationshipStatusCodeType" minOccurs="0" maxOccurs="10">

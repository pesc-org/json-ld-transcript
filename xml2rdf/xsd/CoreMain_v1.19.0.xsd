	<xs:complexType name="SubtestsType">
		<xs:annotation>
			<xs:documentation>A logical grouping of subtest information.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:choice>
				<xs:element name="SubtestCode" type="core:SubtestCodeType">
					<xs:annotation>
						<xs:documentation>Code identifying the sub-part of a standardized test (e.g. Quantitative, Verbal).  These codes are the older codes originally included in the EDI transaction sets.  They are tied to each individual test. </xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="EducationSubtestCode" type="core:EducationSubtestCodeType">
					<xs:annotation>
						<xs:documentation>Code identifying the sub-part of a standardized test (e.g. Quantitative, Verbal).  These codes are the newer codes used in the XML Education Test Score Reporting and the Application for Admission schemas.  They are tied to each individual test.  </xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:choice>
			<xs:element name="SubtestName" type="core:SubtestNameType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Name of the sub-part of a standardized test</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TestScores" type="core:TestScoreType" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>The score received on the test</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="NoteMessage" type="core:NoteMessageType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Additional information about assesssment subtests</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>

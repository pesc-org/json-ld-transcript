						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="xs:decimal">
								<xs:minInclusive value="-9999999.99"/>
								<xs:maxInclusive value="9999999.99"/>
								<xs:totalDigits value="9"/>
								<xs:fractionDigits value="2"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="PaymentToServicerDate" type="xs:date" minOccurs="0">
						<xs:annotation>
							<xs:documentation>The date the Servicer posted the payment to the student's account.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="BookedLoanAmount" type="core:SmallCurrencyType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Total net amount of disbursements accepted and booked.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="BookedLoanAmountDate" type="xs:date" minOccurs="0">
						<xs:annotation>
							<xs:documentation>The date the loan booked on COD.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="EndorserAmount" type="core:SmallCurrencyType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Total amount of loan for which the endorser has agreed to co-sign.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>

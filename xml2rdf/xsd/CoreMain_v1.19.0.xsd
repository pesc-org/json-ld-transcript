			<xs:element name="PhoneNumber" type="core:PhoneNumberType">
				<xs:annotation>
					<xs:documentation>Telephone number</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PhoneNumberExtension" type="core:PhoneNumberExtensionType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The extended telephone addressing information used in addition to phone number such as a PBX extension, a pager PIN, or FAX sub-address</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="EffectiveDate" type="core:AddressEffectiveDateType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>This the date when this phone information for the sending high school did or will become effective.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ExpirationDate" type="core:AddressExpirationDateType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>This the date when this phone information for the sending high school will no longer be valid.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="NoteMessage" type="core:NoteMessageType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Additional information about the phone number of the contact at the source institution</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="URLType">
		<xs:annotation>
			<xs:documentation>The Uniform Resource Locator of source institution</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="URLAddress" type="core:URLAddressType">
				<xs:annotation>
					<xs:documentation>The Uniform Resource Locator (URL) address of the sending high school or of a contact at the sending high school</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="NoteMessage" type="core:NoteMessageType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Additional information about the URL address of the source institution</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="PrivacyRestrictionType">
		<xs:annotation>
			<xs:documentation>Request by the applicant to restrict access to certain data that may normally be released under the US Family Educational Rights and Privacy Act</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="PrivacyRestrictionDate" type="core:PrivacyRestrictionDateType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The date upon which the requested restriction to the allowable release of FERPA directory information takes effect after the applicant has become a student</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PrivacyRestrictionLevel" type="core:PrivacyRestrictionLevelType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The type of restriction placed on the release of information.00  No restrictions have been placed on the release of information 01  Student has requested that no directory information be released without prior consent02  Student has requested nonrelease of specific information items03  Student has requested nonrelease of information in specific formats (book, electronic, etc.)04  Student has requested nonrelease of information to specific people, entities 99  Other  (to be specified in the NoteMessage)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="NoteMessage" type="core:NoteMessageType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Additional information about privacy restriction</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="ContactNameType">
		<xs:annotation>
			<xs:documentation>Name of the contact at the sponsoring organization</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="60"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="ContactTitleType">
		<xs:annotation>
			<xs:documentation>Job title or position of the contact at the sponsoring organization</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="60"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="OkToContactType">
		<xs:annotation>
			<xs:documentation>If known, this is the information that the applicant has provided that it is OK to contact the person at the sponsoring organization about additional information </xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="Yes">
				<xs:annotation>
					<xs:documentation>Yes</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="No">
				<xs:annotation>
					<xs:documentation>No</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>

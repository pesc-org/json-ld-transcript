							<xs:documentation>This tag indicates whether there is an electronic or paper MPN.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="MPNID" minOccurs="0">
						<xs:annotation>
							<xs:documentation>The MPN identifier printed on the MPN.</xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:maxLength value="23"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="MPNStatusCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation>The status of the MPN on file at COD.</xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:enumeration value="A"/>
								<xs:enumeration value="R"/>
								<xs:enumeration value="X"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="MPNLinkIndicator" type="xs:boolean" minOccurs="0">
						<xs:annotation>
							<xs:documentation>The Master Promissory Note indicator is used to indicate whether or not the record has been linked to an MPN at COD.</xs:documentation>
						</xs:annotation>
					</xs:element>

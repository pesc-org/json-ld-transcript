
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix pescDocumentOfficialCodes: <https://pesc.org/vocabs/DocumentOfficialCodes#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .

pescDocumentOfficialCodes:Official a skos:Concept ;
    skos:definition "Official" ;
    skos:inScheme pescDocumentOfficialCodes: ;
    skos:notation "Official" ;
    skos:prefLabel "Official" ;
    skos:topConceptOf pescDocumentOfficialCodes: .

pescDocumentOfficialCodes:Unofficial a skos:Concept ;
    skos:definition "Unofficial" ;
    skos:inScheme pescDocumentOfficialCodes: ;
    skos:notation "Unofficial" ;
    skos:prefLabel "Unofficial" ;
    skos:topConceptOf pescDocumentOfficialCodes: .

pescDocumentOfficialCodes: a skos:ConceptScheme ;
    dcterms:description "This element indicates if the document is unofficial. Unofficial documents may be produced for reference purpose but may not be binding." ;
    dcterms:title "Document Official Codes" ;
    skos:hasTopConcept pescDocumentOfficialCodes:Official,
        pescDocumentOfficialCodes:Unofficial .

